import java.util.Scanner;

public class MaiorPosicao {

    public static void main(String[] args) {
        int N, maior = 0, posicao = 0;
        Scanner sc = new Scanner(System.in);
        for(int i = 1; i <= 100; i++){
            N = sc.nextInt();
            if(N > maior){
                maior = N;
                posicao = i;
            }
		}
		System.out.println(maior);
		System.out.println(posicao);
    }
}
