a=[]
n=5

for i in range(n):
    x=int(input("enter element:"))
    a.append(x)

print("your list input is", a)

b=int(input("enter any element:"))

i=0
for i in range(n):
    if a[i]==b:
        f=1
        break
    else:
        f=0
        

if f==1:
    print("it is there in the list")
else:
    print("it is not there in the list")

